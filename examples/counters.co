(define counter {
	((new)
		(define num (cell new 0))
		{
			((increment n) (num := ((num *) + n)))
			((reset) (num := 0))
			((get) (num *))
		}
	)
})


(define a-counter (counter new))
(define logged-c {
	((get)
		(console print "Gotten!")
		(a-counter get))
	(-> a-counter)
})

(console print (logged-c get))

(logged-c increment 3)
(console print (logged-c get))